{
  "name": "backend",
  "version": "1.0.0",
  "description": "Servidor Backend para aplicacao do desafio",
  "author": "Bruno Raiado Bianchi",
  "main": "./src/main.ts",
  "scripts": {
    "start": "nodemon ./src/main.ts",
    "postgres": " cd../database && docker-compose up -d",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run prettier-format && pnpm run lint",
      "pre-push": ""
    }
  },
  "keywords": [],
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jose": "^6.0.11",
    "nodemon": "^3.1.10",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "0.3.24",
    "typescript": "^5.8.3",
    "uid": "^2.0.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/express": "^5.0.2",
    "@types/node": "^22.13.10",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.32.1"
  }
